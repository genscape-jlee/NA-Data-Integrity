    <DataSet Name="maintenance_status">
      <Query>
        <DataSourceName>genpowerrtdbPROD</DataSourceName>
        <CommandText>select cal_day,region,region_priority_id,plants_999,qt,total_plants,((plants_999/qt)-1)*100 perc_ctq
,(plants_999/(total_plants*.1))*10 perc_999
from (
select cal_day,region,count(case when Conf = 1 then global_entity_id else null end) plants_999, count(*)total_plants,count(*)*.1 qt,
case when region in ('ERCOT','PJM') then 3 
when region in ('CAISO','ISONE','MISO','NYISO','SPP') then 2
when region in ('SERC','WECC') then 1 else null end region_priority_id
from 
(
	SELECT
			es.name as Region
			,ged.GlobalEntityId as Global_Entity_ID
			,e.name as Plant_Name
			,ConfidenceValue Conf
			--,cast(conf.BeginDate AS DATE) as cal_day
			,conf.BeginDate as Reporting_Date_Begin
			,conf.EndDate as Reporting_Date_End
		--	,cast(conf.BeginDate AS DATE) as Reporting_Date_Begin
		--	,cast(conf.EndDate AS DATE) as Reporting_Date_End -- select * 
			,Note,DataProviderCode_Name, DataStreamSourceCode_Name, CollectionSystemTypeCode_Name,
						DATEDIFF(dd, begindate, GETDATE()) date_diff
			FROM entitlement.Entityset es (NOLOCK)
				JOIN entitlement.GenscapeAggregationEntitySetDetail ged (NOLOCK)
						ON es.EntitysetId = ged.EntitysetId
				JOIN entitlement.EntityUniverse e (NOLOCK)
						ON ged.GlobalEntityId = e.GlobalEntityId
				JOIN powerrt.ContentConfidence conf (NOLOCK)
						ON ged.GlobalEntityId = conf.GlobalEntityId
				join masterdata.DimCollectionInstanceType instype (NOLOCK)
						ON instype.DimCollectionInstanceTypeId = conf.CollectionInstanceTypeId
			    join powerrt.powerrt.GlobalEntityDisplayOption gedo (NOLOCK)
						on gedo.GlobalEntityId = ged.GlobalEntityId
		where es.EntitlementEntitysetId in (82487322,82487297,82589091,82487307,82487329,82487337,82487344,82589096,82487466,82589099) -- region id
			AND e.EntityTypeId IN (20, 19) -- PowerPlant, GenerationAggregation
	--		AND conf.ConfidenceValue = 1.00
			AND gedo.IsMonitored = 1 and gedo.ShowInProduct = 1
			AND conf.BeginDate &gt;= DATEADD(YEAR, DATEDIFF(YEAR, 0, DATEADD(year, -2, GETDATE())), 0)
union all

	SELECT
			dg.MenuName GeographyZone
			,e.GlobalEntityId
			,e.Name
			,ConfidenceValue as conf
		--	,cc.begindate
			,cc.begindate
			,cc.enddate
			,Note,DataProviderCode_Name, DataStreamSourceCode_Name, CollectionSystemTypeCode_Name, DATEDIFF(dd, BeginDate, GETDATE()) date_diff
		FROM entitlement.EntityUniverse e  (NOLOCK)
			JOIN masterdata.DimGeographicalZone dg (NOLOCK)
					ON dg.DimGeographicalZoneId = e.GeographyZoneId 
			JOIN powerrt.ContentConfidence cc (NOLOCK)
					ON e.GlobalEntityId = cc.GlobalEntityId 
			join masterdata.DimCollectionInstanceType instype (NOLOCK)
			on instype.DimCollectionInstanceTypeId = cc.CollectionInstanceTypeId
			join powerrt.powerrt.GlobalEntityDisplayOption gedo (NOLOCK)
				on gedo.GlobalEntityId = e.GlobalEntityId
		WHERE e.EntityTypeId IN (19,20)
			AND e.ProductRegionId = 1
			AND gedo.IsMonitored = 1 and gedo.ShowInProduct = 1
			AND e.countryid = 65364955
			AND dg.MenuName IN ('SERC','WECC')
		--	AND cc.ConfidenceValue = 1.00
			AND cc.BeginDate &gt;= DATEADD(YEAR, DATEDIFF(YEAR, 0, DATEADD(year, -2, GETDATE())), 0)
			)a
			inner join 
			(
			SELECT
				CAST(DATEADD(DAY, nbr - 1, '20130101') as date) cal_day
			FROM (
				SELECT ROW_NUMBER() OVER ( ORDER BY c.object_id ) AS Nbr
				FROM sys.columns c
				) nbrs
			WHERE nbr - 1 &lt;= DATEDIFF(DAY, '20170101', '20231001')
				AND DATEADD(DAY, nbr - 1, '20130101')  &lt; GETDATE())b
--ON b.cal_day &gt;= a.Reporting_Date_Begin
ON b.cal_day &gt;= cast(CONVERT(VARCHAR(10), Reporting_Date_Begin, 121) as date)
--				AND b.cal_day &lt;= a.Reporting_Date_End --excludes the last day of the maintenance
				AND b.cal_day &lt; cast(CONVERT(VARCHAR(10), a.Reporting_Date_END, 121) as date) --excludes the last day of the maintenance
--WHERE Reporting_Date_Begin &lt;&gt; Reporting_Date_END -- excludes the plants that were shaded/unshaded on the same day
WHERE cast(CONVERT(VARCHAR(10), Reporting_Date_Begin, 121) as date) &lt;&gt; cast(CONVERT(VARCHAR(10), Reporting_Date_END, 121) as date)-- excludes the plants that were shaded/unshaded on the same day
AND cal_day = CAST(GETDATE() AS DATE)
group by cal_day,region,case when region in ('ERCOT','PJM') then 3 
when region in ('CAISO','ISONE','MISO','NYISO','SPP') then 2
when region in ('SERC','WECC') then 1 else null end )tbl
--order by region_priority_id desc,region

UNION

--TOTAL
select cal_day,region,region_priority_id,plants_999,qt,total_plants,null perc_ctq
,(plants_999/(total_plants*.1))*10 perc_999
from (
select cal_day,'Total'region,count(case when Conf = 1 then global_entity_id else null end) plants_999, count(*)total_plants,count(*)*.1 qt,0 region_priority_id
from 
(select distinct f.* from(
	SELECT
		--	es.name as Region
			ged.GlobalEntityId as Global_Entity_ID
			,e.name as Plant_Name
			,ConfidenceValue Conf
			--,cast(conf.BeginDate AS DATE) as cal_day
			--,cast(conf.BeginDate AS DATE) as Reporting_Date_Begin
			--,cast(conf.EndDate AS DATE) as Reporting_Date_End -- select * 
			,conf.BeginDate as Reporting_Date_Begin
			,conf.EndDate as Reporting_Date_End
			,Note,DataProviderCode_Name, DataStreamSourceCode_Name, CollectionSystemTypeCode_Name,
						DATEDIFF(dd, begindate, GETDATE()) date_diff
			FROM entitlement.Entityset es (NOLOCK)
				JOIN entitlement.GenscapeAggregationEntitySetDetail ged (NOLOCK)
						ON es.EntitysetId = ged.EntitysetId
				JOIN entitlement.EntityUniverse e (NOLOCK)
						ON ged.GlobalEntityId = e.GlobalEntityId
				JOIN powerrt.ContentConfidence conf (NOLOCK)
						ON ged.GlobalEntityId = conf.GlobalEntityId
				join masterdata.DimCollectionInstanceType instype (NOLOCK)
						ON instype.DimCollectionInstanceTypeId = conf.CollectionInstanceTypeId
				join powerrt.powerrt.GlobalEntityDisplayOption gedo (NOLOCK)
						on gedo.GlobalEntityId = ged.GlobalEntityId
		where es.EntitlementEntitysetId in (82487322,82487297,82589091,82487307,82487329,82487337,82487344,82589096,82487466,82589099) -- region id
			AND e.EntityTypeId IN (20, 19) -- PowerPlant, GenerationAggregation
			AND gedo.IsMonitored = 1 and gedo.ShowInProduct = 1
	--		AND conf.ConfidenceValue = 1.00
	--		AND conf.BeginDate &gt;= DATEADD(YEAR, DATEDIFF(YEAR, 0, DATEADD(year, -1, GETDATE())), 0)
	--		AND conf.BeginDate &gt;= GETDATE()-365
union all

	SELECT
		--	dg.MenuName GeographyZone
			e.GlobalEntityId
			,e.Name
			,ConfidenceValue as conf
		--	,cc.begindate
		--	,cast(CONVERT(VARCHAR(10), cc.begindate, 121) as date) as begindate
		--    ,cast(CONVERT(VARCHAR(10), cc.enddate, 121) as date) as enddate
			,cc.BeginDate as Reporting_Date_Begin
			,cc.EndDate as Reporting_Date_End
			,Note,DataProviderCode_Name, DataStreamSourceCode_Name, CollectionSystemTypeCode_Name, DATEDIFF(dd, BeginDate, GETDATE()) date_diff
		FROM entitlement.EntityUniverse e  (NOLOCK)
			JOIN masterdata.DimGeographicalZone dg (NOLOCK)
					ON dg.DimGeographicalZoneId = e.GeographyZoneId 
			JOIN powerrt.ContentConfidence cc (NOLOCK)
					ON e.GlobalEntityId = cc.GlobalEntityId 
			join masterdata.DimCollectionInstanceType instype (NOLOCK)
			on instype.DimCollectionInstanceTypeId = cc.CollectionInstanceTypeId
			join powerrt.powerrt.GlobalEntityDisplayOption gedo (NOLOCK)
				on gedo.GlobalEntityId = e.GlobalEntityId
		WHERE e.EntityTypeId IN (19,20)
			AND e.ProductRegionId = 1
			AND e.countryid = 65364955
			AND gedo.IsMonitored = 1 and gedo.ShowInProduct = 1
			AND dg.MenuName IN ('SERC','WECC')
			)f
		--	AND cc.ConfidenceValue = 1.00
		--	AND cc.BeginDate &gt;= DATEADD(YEAR, DATEDIFF(YEAR, 0, DATEADD(year, -1, GETDATE())), 0)
		--	AND cc.BeginDate &gt;= GETDATE()-365
			)a
			
			inner join 
			(
			SELECT
				CAST(DATEADD(DAY, nbr - 1, '20130101') as date) cal_day
			FROM (
				SELECT ROW_NUMBER() OVER ( ORDER BY c.object_id ) AS Nbr
				FROM sys.columns c
				) nbrs
			WHERE nbr - 1 &lt;= DATEDIFF(DAY, '20170101', '20231001')
				AND DATEADD(DAY, nbr - 1, '20130101')  &lt; GETDATE())b
--ON b.cal_day &gt;= a.Reporting_Date_Begin
ON b.cal_day &gt;= cast(CONVERT(VARCHAR(10), Reporting_Date_Begin, 121) as date)
--				AND b.cal_day &lt;= a.Reporting_Date_End -1 --excludes the last day of the maintenance
				AND b.cal_day &lt; cast(CONVERT(VARCHAR(10), a.Reporting_Date_END, 121) as date) --excludes the last day of the maintenance
--WHERE Reporting_Date_Begin &lt;&gt; Reporting_Date_END -- excludes the plants that were shaded/unshaded on the same day
WHERE cast(CONVERT(VARCHAR(10), Reporting_Date_Begin, 121) as date) &lt;&gt; cast(CONVERT(VARCHAR(10), Reporting_Date_END, 121) as date)-- excludes the plants that were shaded/unshaded on the same day
AND cal_day = CAST(GETDATE() AS DATE)
group by cal_day)tbl
order by region_priority_id desc,region;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="cal_day">
          <DataField>cal_day</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="region">
          <DataField>region</DataField>
