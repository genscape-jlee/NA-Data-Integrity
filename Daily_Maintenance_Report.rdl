    <DataSet Name="plant_by_region">
      <Query>
        <DataSourceName>genpowerrtdbPROD</DataSourceName>
        <CommandText>WITH entities --(Region,Global_Entity_ID,Plant_Name,cal_day,Reporting_Date_Begin,Reporting_Date_End,Note,DataProviderCode_Name,DataStreamSourceCode_Name,date_diff)
AS(
		SELECT
			es.name as Region
			,ged.GlobalEntityId as Global_Entity_ID
			,e.name as Plant_Name
			--,cast(conf.BeginDate AS DATE) as cal_day
			,cast(conf.BeginDate AS DATE) as Reporting_Date_Begin
			,cast(conf.EndDate AS DATE) as Reporting_Date_End -- select * 
			,Note,DataProviderCode_Name, DataStreamSourceCode_Name, CollectionSystemTypeCode_Name,
						DATEDIFF(dd, begindate, GETDATE()) date_diff
			FROM entitlement.Entityset es (NOLOCK)
				JOIN entitlement.GenscapeAggregationEntitySetDetail ged (NOLOCK)
						ON es.EntitysetId = ged.EntitysetId
				JOIN entitlement.EntityUniverse e (NOLOCK)
						ON ged.GlobalEntityId = e.GlobalEntityId
				JOIN powerrt.ContentConfidence conf (NOLOCK)
						ON ged.GlobalEntityId = conf.GlobalEntityId
				join masterdata.DimCollectionInstanceType instype (NOLOCK)
						ON instype.DimCollectionInstanceTypeId = conf.CollectionInstanceTypeId
			    join powerrt.powerrt.GlobalEntityDisplayOption gedo (NOLOCK)
						on gedo.GlobalEntityId = ged.GlobalEntityId
		where es.EntitlementEntitysetId in (82487322,82487297,82589091,82487307,82487329,82487337,82487344,82589096,82487466,82589099) -- region id
			AND e.EntityTypeId IN (20, 19) -- PowerPlant, GenerationAggregation
			AND conf.ConfidenceValue = 1.00
	--		AND gedo.IsMonitored = 1 and gedo.ShowInProduct = 1
			AND conf.BeginDate &gt;= DATEADD(YEAR, DATEDIFF(YEAR, 0, DATEADD(year, -2, GETDATE())), 0)
			AND DATEDIFF(dd, begindate, GETDATE()) &lt; 180
		UNION ALL

		SELECT
			dg.MenuName GeographyZone
			,e.GlobalEntityId
			,e.Name
		--	,cc.begindate
			,cc.begindate
			,cc.enddate
			,Note,DataProviderCode_Name, DataStreamSourceCode_Name, CollectionSystemTypeCode_Name, DATEDIFF(dd, BeginDate, GETDATE()) date_diff
		FROM entitlement.EntityUniverse e  (NOLOCK)
			JOIN masterdata.DimGeographicalZone dg (NOLOCK)
					ON dg.DimGeographicalZoneId = e.GeographyZoneId 
			JOIN powerrt.ContentConfidence cc (NOLOCK)
					ON e.GlobalEntityId = cc.GlobalEntityId 
			join masterdata.DimCollectionInstanceType instype (NOLOCK)
			on instype.DimCollectionInstanceTypeId = cc.CollectionInstanceTypeId
			join powerrt.powerrt.GlobalEntityDisplayOption gedo (NOLOCK)
						on e.GlobalEntityId = gedo.GlobalEntityId
		WHERE e.EntityTypeId IN (19,20)
			AND e.ProductRegionId = 1
			AND e.countryid = 65364955
			AND gedo.IsMonitored = 1 and gedo.ShowInProduct = 1
			AND dg.MenuName IN ('FRCC','SERC','WECC')
			AND cc.ConfidenceValue = 1.00
			AND cc.BeginDate &gt;= DATEADD(YEAR, DATEDIFF(YEAR, 0, DATEADD(year, -2, GETDATE())), 0)
			AND DATEDIFF(dd, begindate, GETDATE()) &lt; 180
	)
	,TimeSeries AS(
			SELECT
				CAST(DATEADD(DAY, nbr - 1, '20130101') as date) cal_day
			FROM (
				SELECT ROW_NUMBER() OVER ( ORDER BY c.object_id ) AS Nbr
				FROM sys.columns c
				) nbrs
			WHERE nbr - 1 &lt;= DATEDIFF(DAY, '20170101', '20231001')
				AND DATEADD(DAY, nbr - 1, '20130101')  &lt; GETDATE()
	)
	, R as (select CAST(GETDATE() AS DATE)dt, name region from entitlement.Entityset es (nolock)
where es.EntitlementEntitysetId in (82487322,82487297,82589091,82487307,82487329,82487337,82487344,82589096,82487466,82589099))
select R.dt,R.region,
case when Global_Entity_ID is null then 0 else Global_Entity_ID end Global_Entity_ID 
,case when Plant_Name is null then 'No plants on 999 in this region' else Plant_Name end Plant_Name
,case when Reporting_Date_Begin is null then 'No plants on 999 in this region' else CONVERT(varchar(10),Reporting_Date_Begin,101) end Reporting_Date_Begin 
,case when Reporting_Date_End is null then 'No plants on 999 in this region' else CONVERT(varchar(10),Reporting_Date_End,101) end Reporting_Date_End  
--,case when Note is null then 'No plants on 999 in this region' else Note end Note  
,Note
--,case when DataProviderCode_Name is null then 'No plants on 999 in this region' else DataProviderCode_Name end DataProviderCode_Name
--,case when DataStreamSourceCode_Name is null then 'No plants on 999 in this region' else DataStreamSourceCode_Name end DataStreamSourceCode_Name
--,case when CollectionSystemTypeCode_Name is null then 'No plants on 999 in this region' else CollectionSystemTypeCode_Name end CollectionSystemTypeCode_Name
,DataProviderCode_Name
,DataStreamSourceCode_Name
,CollectionSystemTypeCode_Name
,case when date_diff is null then 0 else date_diff end date_diff
 from 
R left outer join

	(SELECT DISTINCT
		TS.cal_day
,case when E.region is null then 'N/A' else E.Region end Region
,Global_Entity_ID,Plant_Name,Reporting_Date_Begin,Reporting_Date_End,Note,DataProviderCode_Name,DataStreamSourceCode_Name,CollectionSystemTypeCode_Name,date_diff
	FROM  entities E
	inner join TimeSeries TS
	ON TS.cal_day &gt;= E.Reporting_Date_Begin
AND TS.cal_day &lt;= E.Reporting_Date_End - 1--excludes the last day of the maintenance
WHERE Reporting_Date_Begin &lt;&gt; Reporting_Date_END -- excludes the plants that were shaded/unshaded on the same day
AND cal_day = CAST(GETDATE() AS DATE)
)a
on dt = cal_day and R.region = a.region
ORDER BY dt, region;
</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="dt">
          <DataField>dt</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="region">
          <DataField>region</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Global_Entity_ID">
          <DataField>Global_Entity_ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Plant_Name">
          <DataField>Plant_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Reporting_Date_Begin">
          <DataField>Reporting_Date_Begin</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Reporting_Date_End">
          <DataField>Reporting_Date_End</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Note">
          <DataField>Note</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DataProviderCode_Name">
          <DataField>DataProviderCode_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="DataStreamSourceCode_Name">
          <DataField>DataStreamSourceCode_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CollectionSystemTypeCode_Name">
          <DataField>CollectionSystemTypeCode_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="date_diff">
          <DataField>date_diff</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="maintenance_history">
      <Query>
        <DataSourceName>genpowerrtdbPROD</DataSourceName>
        <CommandText>WITH entities --(Region,Global_Entity_ID,Plant_Name,cal_day,Reporting_Date_Begin,Reporting_Date_End,Note,DataProviderCode_Name,DataStreamSourceCode_Name,date_diff)
AS(
		SELECT
			es.name as Region
			,ged.GlobalEntityId as Global_Entity_ID
			,e.name as Plant_Name
			--,cast(conf.BeginDate AS DATE) as cal_day
			,cast(conf.BeginDate AS DATE) as Reporting_Date_Begin
			,cast(conf.EndDate AS DATE) as Reporting_Date_End -- select * 
			,Note,DataProviderCode_Name, DataStreamSourceCode_Name, DATEDIFF(dd, begindate, GETDATE()) date_diff
			FROM entitlement.Entityset es (NOLOCK)
				JOIN entitlement.GenscapeAggregationEntitySetDetail ged (NOLOCK)
						ON es.EntitysetId = ged.EntitysetId
				JOIN entitlement.EntityUniverse e (NOLOCK)
						ON ged.GlobalEntityId = e.GlobalEntityId
				JOIN powerrt.ContentConfidence conf (NOLOCK)
						ON ged.GlobalEntityId = conf.GlobalEntityId
				join masterdata.DimCollectionInstanceType instype (NOLOCK)
						ON instype.DimCollectionInstanceTypeId = conf.CollectionInstanceTypeId
		where es.EntitlementEntitysetId in (82487322,82487297,82589091,82487307,82487329,82487337,82487344,82589096,82487466,82589099) -- region id
			AND e.EntityTypeId IN (20, 19) -- PowerPlant, GenerationAggregation
			AND conf.ConfidenceValue = 1.00
			AND conf.BeginDate &gt;= DATEADD(YEAR, DATEDIFF(YEAR, 0, DATEADD(year, -2, GETDATE())), 0)
			
		UNION ALL

		SELECT
			dg.MenuName GeographyZone
			,e.GlobalEntityId
			,e.Name
		--	,cc.begindate
			,cc.begindate
			,cc.enddate
			,Note,DataProviderCode_Name, DataStreamSourceCode_Name, DATEDIFF(dd, BeginDate, GETDATE()) date_diff
		FROM entitlement.EntityUniverse e  (NOLOCK)
			JOIN masterdata.DimGeographicalZone dg (NOLOCK)
					ON dg.DimGeographicalZoneId = e.GeographyZoneId 
			JOIN powerrt.ContentConfidence cc (NOLOCK)
					ON e.GlobalEntityId = cc.GlobalEntityId 
			join masterdata.DimCollectionInstanceType instype (NOLOCK)
			on instype.DimCollectionInstanceTypeId = cc.CollectionInstanceTypeId
		WHERE e.EntityTypeId IN (19,20)
			AND e.ProductRegionId = 1
			AND e.countryid = 65364955
			AND dg.MenuName IN ('FRCC','SERC','WECC')
			AND cc.ConfidenceValue = 1.00
			AND cc.BeginDate &gt;= DATEADD(YEAR, DATEDIFF(YEAR, 0, DATEADD(year, -2, GETDATE())), 0)
	)
	,TimeSeries AS(
			SELECT
				CAST(DATEADD(DAY, nbr - 1, '20130101') as date) cal_day
			FROM (
				SELECT ROW_NUMBER() OVER ( ORDER BY c.object_id ) AS Nbr
				FROM sys.columns c
				) nbrs
			WHERE nbr - 1 &lt;= DATEDIFF(DAY, '20170101', '20231001')
				AND DATEADD(DAY, nbr - 1, '20130101')  &lt; GETDATE()
	)
	SELECT DISTINCT
		TS.cal_day
		,E.Region
		,E.Global_Entity_ID
		,E.Plant_Name
		,E.Reporting_Date_Begin
		,E.Reporting_Date_End 
		,E.DataProviderCode_Name
		,E.DataStreamSourceCode_Name
		,E.date_diff
		,E.Note
	FROM TimeSeries TS
		INNER JOIN entities E
				ON TS.cal_day &gt;= E.Reporting_Date_Begin
				AND TS.cal_day &lt;= E.Reporting_Date_End - 1 --excludes the last day of the maintenance
WHERE Reporting_Date_Begin &lt;&gt; Reporting_Date_END -- excludes the plants that were shaded/unshaded on the same day
--AND cal_day = CAST(GETDATE() AS DATE)
ORDER BY cal_day, reporting_date_begin DESC;</CommandText>
      </Query>
      <Fields>
